  EMPLOYEES1.HIRE_DATE HIRE_DATE1,
  EMPLOYEES1.JOB_ID JOB_ID1,
  EMPLOYEES1.SALARY SALARY1,
  EMPLOYEES1.COMMISSION_PCT COMMISSION_PCT1,
  EMPLOYEES1.MANAGER_ID MANAGER_ID1,
  EMPLOYEES1.DEPARTMENT_ID DEPARTMENT_ID1,
